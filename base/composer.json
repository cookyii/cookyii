{
  "name": "cookyii/base",
  "description": "Base application for Cookyii CMF",
  "keywords": [
    "yii2",
    "framework",
    "cookee",
    "cookyii",
    "cmf"
  ],
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Revin Roman",
      "email": "roman@rmrevin.ru",
      "homepage": "https://rmrevin.ru/",
      "role": "Founder and project lead"
    }
  ],
  "support": {
    "issues": "https://github.com/cookyii/cookyii/issues?state=open",
    "source": "https://github.com/cookyii/cookyii"
  },
  "require": {
    "php": ">=5.5.0",

    "yiisoft/yii2": "2.0.4",
    "yiisoft/yii2-composer": "2.0.3",
    "yiisoft/yii2-bootstrap": "2.0.4",
    "yiisoft/yii2-imagine": "2.0.3",

    "rmrevin/yii2-fontawesome": "2.10.1",
    "rmrevin/yii2-socicon": "1.0.0",
    "rmrevin/yii2-minify-view": "1.8.0",
    "rmrevin/yii2-rbac-command": "1.1.3",
    "rmrevin/yii2-power-migration": "1.1.2",
    "rmrevin/yii2-postman": "2.2.4"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "2.0.4",
    "yiisoft/yii2-faker": "2.0.3",
    "yiisoft/yii2-codeception": "2.0.4",
    "codeception/codeception": "2.0.*",
    "codeception/phpbuiltinserver": "1.2.*",
    "codeception/specify": "0.4.*",
    "codeception/verify": "0.2.*"
  },
  "autoload": {
    "psr-4": {"cookyii\\": ""}
  },
  "bin": [
    "yii"
  ],
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}