{
  "name": "cookyii/project",
  "description": "Base application for Cookyii CMF",
  "keywords": [
    "yii2",
    "framework",
    "cookie",
    "cookyii",
    "cmf"
  ],
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Revin Roman",
      "email": "roman@rmrevin.ru",
      "homepage": "https://rmrevin.ru/",
      "role": "Founder and project lead"
    }
  ],
  "support": {
    "issues": "https://github.com/cookyii/project/issues?state=open",
    "source": "https://github.com/cookyii/project"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "replace": {
    "cookyii/base": "self.version",
    "cookyii/module-feed": "self.version",
    "cookyii/module-account": "self.version",
    "cookyii/module-client": "self.version",
    "cookyii/module-page": "self.version",
    "cookyii/module-postman": "self.version",
    "cookyii/module-media": "self.version"
  },
  "require": {
    "php": ">=5.4.0",

    "cookyii/build": "dev-master",
    "cookyii/base": "dev-master",
    "cookyii/module-feed": "dev-master",
    "cookyii/module-account": "dev-master",
    "cookyii/module-client": "dev-master",
    "cookyii/module-page": "dev-master",
    "cookyii/module-postman": "dev-master",
    "cookyii/module-media": "dev-master",

    "yiisoft/yii2": "2.0.6",
    "yiisoft/yii2-composer": "2.0.*",
    "yiisoft/yii2-bootstrap": "2.0.*",
    "yiisoft/yii2-imagine": "2.0.*",
    "yiisoft/yii2-authclient": "2.0.*",
    "yiisoft/yii2-swiftmailer": "2.0.*",

    "rmrevin/yii2-fontawesome": "~2.10",
    "rmrevin/yii2-socicon": "~1.0",
    "rmrevin/yii2-minify-view": "~1.9",
    "rmrevin/yii2-rbac-command": "~1.3",
    "rmrevin/yii2-power-migration": "~1.1",
    "rmrevin/yii2-imperavi-bundle": "~1.0",
    "rmrevin/yii2-rollbar": "~1.1",
    "rmrevin/yii2-favicon-widget": "~1.0",

    "cboden/ratchet": "0.3.*",
    "samdark/sitemap": "2.0.*",
    "vlucas/phpdotenv": "2.0.*"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "2.0.*",
    "yiisoft/yii2-faker": "2.0.*",
    "yiisoft/yii2-codeception": "2.0.*",
    "codeception/codeception": "2.0.*",
    "codeception/phpbuiltinserver": "1.2.*",
    "codeception/specify": "0.4.*",
    "codeception/verify": "0.2.*"
  },
  "autoload": {
    "psr-4": {
      "dev\\": "./dev",
      "frontend\\": "./frontend-app",
      "frontend\\modules\\": "./frontend-modules",
      "backend\\": "./backend-app",
      "backend\\modules\\": "./backend-modules",
      "crm\\": "./crm-app",
      "crm\\modules\\": "./crm-modules",
      "cookyii\\modules\\Feed\\": "./modules/Feed",
      "cookyii\\modules\\Account\\": "./modules/Account",
      "cookyii\\modules\\Client\\": "./modules/Client",
      "cookyii\\modules\\Order\\": "./modules/Order",
      "cookyii\\modules\\Page\\": "./modules/Page",
      "cookyii\\modules\\Postman\\": "./modules/Postman",
      "cookyii\\modules\\Media\\": "./modules/Media",
      "cookyii\\": "./components",
      "resources\\": [
        "./resources",
        "./modules/Feed/resources",
        "./modules/Account/resources",
        "./modules/Client/resources",
        "./modules/Order/resources",
        "./modules/Page/resources",
        "./modules/Postman/resources",
        "./modules/Media/resources"
      ]
    }
  },
  "extra": {
    "writable": [
      "frontend-app/runtime",
      "frontend-app/web/assets",
      "frontend-app/web/storage",
      "frontend-app/web/upload",
      "backend-app/runtime",
      "backend-app/web/assets",
      "backend-app/web/storage",
      "backend-app/web/upload",
      "crm-app/runtime",
      "crm-app/web/assets",
      "crm-app/web/storage",
      "crm-app/web/upload"
    ],
    "executable": [
      "build",
      "codecept",
      "generate_self_signed_ssl_key",
      "frontend",
      "backend",
      "crm"
    ],
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}